<?xml version="1.0"?>
<launch>
		<node pkg="gmapping" type="slam_gmapping" name="slam_gmapping">
			<remap from="scan" to ="/sensor/scan" />
			<rosparam command="load" file="$(find sugv_navigation)/config/gmapping.yaml"/>
		</node>

		<!-- <include file="$(find amcl)/examples/amcl_diff.launch" /> -->

		<node pkg="amcl" type="amcl" name="amcl" output="screen">
			<rosparam file="$(find sugv_navigation)/config/amcl.yaml"/>
		</node>

		<node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
			<rosparam file="$(find sugv_navigation)/config/costmap_common.yaml" command="load" ns="global_costmap" />
			<rosparam file="$(find sugv_navigation)/config/costmap_common.yaml" command="load" ns="local_costmap" />
			<rosparam file="$(find sugv_navigation)/config/local_costmap.yaml" command="load" />
			<rosparam file="$(find sugv_navigation)/config/global_costmap.yaml" command="load" />
			<rosparam file="$(find sugv_navigation)/config/base_local_planner.yaml" command="load" />
		</node>
</launch>